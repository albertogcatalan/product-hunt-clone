{% set langCurrent = language.locale|split('_') %}
<!DOCTYPE html>
<html lang="{{ langCurrent[0] }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" type="image" href="{{ qv.img }}/favicon.png">

    <title>{% block title %}{% endblock %}</title>

    <!-- CSS -->
    <link href="{{ qv.css }}/bootstrap.min.css?{{ qv.randomVar }}" rel="stylesheet">
    <link href="{{ qv.css }}/admin.min.css?{{ qv.randomVar }}" rel="stylesheet">

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
