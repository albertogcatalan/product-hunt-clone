   {% block body %}

    {% block sidebar %}
  
      {% include 'aSidebar.twig' %}

    {% endblock %}

    {% block content %}
  
      

    {% endblock %}
  {% endblock %}
  </div>
  </div>

  {% block footer %}{% endblock %}
    
    
    

    {% block JS_Footer %}{% endblock %}
  </body>
</html>
